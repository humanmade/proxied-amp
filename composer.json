{
	"name": "humanmade/proxied-amp",
	"description": "Validate AMP Pages on Proxied Website",
	"type": "project",
	"license": "GPL-2.0-or-later",
	"config": {
		"platform": {
			"php": "7.2"
		},
		"preferred-install": "dist",
		"process-timeout": 600,
		"sort-packages": true
	},
	"require": {
		"php": ">=7.2",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"humanmade/coding-standards": "^0.8.0",
		"phpunit/phpunit": "^7.0"
	},
	"scripts": {
		"lint": "phpcs .",
		"test": "docker run --rm -v $PWD:/code humanmade/plugin-tester"
	}
}
